using System;

class Program
{
    static void Main(string[] args)
    {

        int a = 10;
        int b = 38;
        int c = (31 - 5 * a) / b;
        Console.WriteLine(c);
        /*
         * 5*10=50, 50-31=19, 19/38=0,5
         * int целочисленный тип.
         * ответ int c = 0;
         */
    }
}
